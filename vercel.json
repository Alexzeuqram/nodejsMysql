{
    "builds": [
      {
        "src": "./index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "src": "/articulos/:ultimos?", 
        "dest": "/index.js"
      },
      { 
        "src": "/articulo/:id", 
        "dest": "/index.js"
      },
      { 
        "src": "/crear", 
        "dest": "/index.js"
      },
      { 
        "src": "/articulo/:id", 
        "dest": "/index.js"
      },
      { 
        "src": "/subir-imagen/:id", 
        "dest": "/index.js"
      },
      { 
        "src": "/imagen/:nombreFichero", 
        "dest": "/index.js"
      },
      { 
        "src": "/buscar/:busqueda", 
        "dest": "/index.js"
      },
      { 
        "src": "/.*", 
        "dest": "/index.js"
      }
    ]
  }
  